{
  "name": "setup-python-project",
  "dockerComposeFile": [
    "../docker-compose.yaml",
    "docker-compose-override.yaml"
  ],
  "service": "app",
  "workspaceFolder": "/app",
  "extensions": [
    "bungcip.better-toml",
    "davidanson.vscode-markdownlint",
    "EditorConfig.EditorConfig",
    "ms-python.python",
    "njpwerner.autodocstring",
    "redhat.vscode-yaml"
  ],
  "postCreateCommand": "pipenv install --dev && python -m pip install --upgrade pip && pre-commit install"
}
